<!-- Common atributes for all xmls-->
<root>
    <type><type>
    <!-- 
        STATUSES:
        - OK
        - ERR
    -->
    <status></status>
    <!-- Describes the error, empty if status is OK-->
    <error></error>
</root>

<!-- Server response -->
<root>
    <type><type>
    <status></status>
    <error></error>
</root>


<!-- Create user -->
<root>
    <type><type>
    <status></status>
    <error></error>
    
    <name></name>
    <password></password>
</root>
